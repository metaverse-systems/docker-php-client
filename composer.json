{
    "name": "metaverse-systems/docker-php-client",
    "description": "PHP client for Docker Engine REST API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MetaverseSystems\\DockerPhpClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tim Schwartz",
            "email": "tim@metaverse.systems"
        }
